{
  "manifest_version": 2,

  "name": "Simple Calculator",
  "description": "This extension allows you to do calculations within chrome",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "calculator.html",
    "default_title": "calculator"
  },
  "permissions": [
    "activeTab"
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  }
}